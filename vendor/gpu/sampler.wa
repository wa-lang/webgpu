
import "js"

/*
不能使用该类型直接声明值，需通过 Device.CreateSampler() 创建
*/
type Sampler struct {
    js.ExtObj
}

/*
不能使用该类型直接声明值，NewSamplerDesc() 创建
*/
type SamplerDesc struct {
    js.ExtObj
}

func NewSamplerDesc() => SamplerDesc {
    d: SamplerDesc
    d.ExtObj = js.NewExtObj()
    return d
}

type Filter string

const (
    Filter_linear = "linear"
)

func SamplerDesc.SetMagFilter(filter: string) {
    println(filter)
    this.SetMember_string("magFilter", filter)
}

func SamplerDesc.SetMinFilter(filter: string) {
    this.SetMember_string("minFilter", filter)
}